{
  "manifest_version": 3,
  "name": "DDL & JSON to Go Struct",
  "version": "1.0.0",
  "description": "Convert database DDL (MySQL, PostgreSQL, SQLite) and JSON to Go structs with gorm tags",
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "downloads"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}